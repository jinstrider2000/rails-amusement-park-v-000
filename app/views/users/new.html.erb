<h1>Register</h1>
<%= form_for @user do |f| %>
  <%= f.label :name %>
  <%= f.text_field :name %>
  <br>
  <%= f.label :tickets %>
  <%= f.text_field :tickets %>
  <br>
  <%= f.label :height %>
  <%= f.text_field :height %>
  <br>
  <%= f.label :nausea %>
  <%= f.text_field :nausea %>
  <br>
  <%= f.label :happiness %>
  <%= f.text_field :happiness %>
  <br>
  <%= f.label :admin, "Administrator?" %>
  <%= f.check_box :admin %>
  <br>
  <%= f.submit %>
<% end %>
