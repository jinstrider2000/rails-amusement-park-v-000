<h1>Register</h1>
<%= form_for @user do |f| %>
  <%= f.label :name %>
  <%= f.text_field :name %>
  <br>
  <%= f.label :password %>
  <%= f.password_field :password %>
  <br>
  <%= f.label :tickets %>
  <%= f.text_field :tickets %>
  <br>
  <%= f.label :height %>
  <%= f.text_field :height %>
  <br>
  <%= f.label :nausea %>
  <%= f.text_field :nausea %>
  <br>
  <%= f.label :happiness %>
  <%= f.text_field :happiness %>
  <br>
  <%= f.label :created_at %>
  <%= f.date_select :created_at, default: Date.today, order: [:month, :day, :year], date_separator: "/ ", start_year: Date.today.year - 20, end_year: Date.today.year %>
  <br>
  <%= f.label :admin, "Administrator?" %>
  <%= f.check_box :admin %>
  <br>
  <%= f.submit %>
<% end %>
